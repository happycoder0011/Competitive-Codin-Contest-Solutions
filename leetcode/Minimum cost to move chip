class Solution {
public:
    int minCostToMoveChips(vector<int>& position) {
     int even=0,odd=0;
        for(int i:position)
     {
         if(i%2==0)
             even++;
         else
             odd++;
     }
        
    int mi = min(even,odd);
        
        
        
        return mi;
    }
};
