class Solution {
public:
    int arraySign(vector<int>& nums) {
        int d=0;
        for(int i : nums)
        {
            if(i<0)
                d++;
            
            if(i==0)
                return 0;
        }
        if(d%2==0)
            return 1;
        else
            return -1;
    }
};
