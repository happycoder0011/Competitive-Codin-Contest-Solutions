class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& A) {
        vector<int> odd;
            for(int i=0;i<A.size();i++)
            {
                    if(A[i]%2!=0)
                    {
                            odd.push_back(A[i]);
                    }
                    else
                            odd.insert(odd.begin(),A[i]);
            }
           
            return odd;
    }
       
};
